{
  "manifest_version": 2,
  "name": "Ebay Fixer",
  "version": "1.0",
  "description": "Fixes some general issues with eBay's website",
  "icons": {

  },
  "background":
  {
    "scripts": ["import/jquery.js", "index.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.ebay.ca/*"],
      "js": ["index.js"],
      "run_at":"document_end"
    }
  ]
}